# todo: move to std
try_call {
  dup block_exists? if {
    call
  }
}

try_call_or {
  dup block_exists? if {
    call
    return
  }
  drop call
}

hello_world {
  "hello, world!\n" ena.io.print
}

main {
  'hello_world try_call
  'nonexistent_block try_call
  'hello_world 'nonexistent_block try_call_or
}